{
  "expo": {
    "owner": "rum-pirates",
    "name": "rum-compass",
    "slug": "rum-compass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/square_compass.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.whereistherum.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A compass that doesn't point North. But we're not trying to find North now are we?"
  }
}
